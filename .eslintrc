{
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb-base/legacy",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/flowtype"
  ],
  "plugins": [
    "import",
    "flowtype",
    "prettier",
    "jest"
  ],
  "rules": {
    "vars-on-top": "off",
    "prettier/prettier": "error"
  }
}
