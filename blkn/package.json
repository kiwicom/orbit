{
  "name": "@kiwicom/balkan-ui",
  "version": "0.0.0-rc2",
  "scripts": {
    "storybook": "start-storybook -p 6007 -c .storybook",
    "build": "babel --out-dir lib --ignore *.stories.js,*.test.js src",
    "build:flow": "flow-copy-source -v -i '**/*.stories.js,**/*.test.js' src lib",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build && npm run build:flow"
  },
  "keywords": ["blkn", "ui", "library", "component"],
  "main": "lib/index.js",
  "author": "kiwi.com",
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-jsx": "^2.2.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/react": "^3.3.13",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "faker": "^4.1.0",
    "flow-bin": "^0.65.0",
    "flow-copy-source": "^1.3.0",
    "jest": "^22.3.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2"
  }
}
