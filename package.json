{
  "name": "@kiwicom/orbit-design-tokens",
  "version": "0.0.10",
  "description": "Design tokens for kiwi!",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwicom/orbit-design-tokens.git"
  },
  "scripts": {
    "prepublish": "node generate-theo",
    "build-html": "theo --setup ./theo/setup.js theo-spec.json --format kiwi.html --dest ./pages",
    "build-web": "theo theo-spec.json --transform web --format json,scss,sass,less,styl,cssmodules.css,common.js --dest ./output",
    "build-ios": "theo theo-spec.json --transform ios --format ios.json --dest ./output",
    "build-android": "theo theo-spec.json --transform android --format android.xml --dest ./output",
    "build": "npm run build-html && npm run build-web && npm run build-ios && npm run build-android",
    "ci-publish": "ci-publish"
  },
  "author": "kiwi.com",
  "license": "MIT",
  "keywords": [
    "orbit",
    "ui",
    "design"
  ],
  "devDependencies": {
    "babylon": "^6.18.0",
    "theo": "^7.0.1",
    "ci-publish": "^1.3.1"
  }
}
