bleBody>\n              <TableRow>\n                <TableCell>\n                  <Icons.BaggageCabin /> Cabin baggage\n                </TableCell>\n                <TableCell>Free</TableCell>\n              </TableRow>\n              <TableRow>\n                <TableCell>\n                  <Icons.BaggageChecked /> Checked baggage\n                </TableCell>\n                <TableCell>Paid-for option</TableCell>\n              </TableRow>\n            </TableBody>\n          </Table>\n          <Table type=\"secondary\">\n            <TableHead>\n              <TableRow>\n                <TableCell as=\"th\">Baggage</TableCell>\n                <TableCell as=\"th\">Availability</TableCell>\n              </TableRow>\n            </TableHead>\n            <TableBody>\n              <TableRow>\n                <TableCell>\n                  <Icons.BaggageCabin /> Cabin baggage\n                </TableCell>\n                <TableCell>Free</TableCell>\n              </TableRow>\n              <TableRow>\n                <TableCell>\n                  <Icons.BaggageChecked /> Checked baggage\n                </TableCell>\n                <TableCell>Paid-for option</TableCell>\n              </TableRow>\n            </TableBody>\n          </Table>\n        </Stack>\n      </Stack>\n    </Stack>\n  </Stack>\n)\n",
