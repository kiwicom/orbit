{
  "name": "@kiwicom/orbit-components",
  "version": "1.2.2",
  "description": "Orbit-components is a React component library which provides developers with the easiest possible way of building Kiwi.comâ€™s products.",
  "sideEffects": false,
  "scripts": {
    "storybook": "start-storybook -p 6007 -c .storybook --ci -s ./static",
    "cy:dev": "vite cypress/integration --logLevel silent",
    "cy:run": "start-server-and-test cy:dev http-get://localhost:3000 'cypress run'",
    "cy:open": "start-server-and-test cy:dev http-get://localhost:3000 'cypress open'",
    "prepublishOnly": "yarn build",
    "build": "zx config/build.mjs",
    "check:icons": "babel-node config/checkIconsCli.js",
    "build:iconsPng": "node config/generatePngIcons.js",
    "build:storybook": "build-storybook -c .storybook -o .out -s ./static",
    "build:examples": "babel-node config/buildExample.js 'src/**/__examples__/*'",
    "eslint:check": "eslint . --report-unused-disable-directives",
    "fetch-translations": "babel-node config/fetchTranslations.js",
    "deploy:storybook": "yarn storybook-to-ghpages -e ./.out",
    "deploy:surge": "yarn surge .out/",
    "deploy:updateURL": "yarn babel-node config/deploymentUtils.js updateLiveURL",
    "compile:ts": "yarn tsc",
    "docs-gen": "monstra docs"
  },
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "hello@ghbot.com",
    "commitMessage": "Deploy Storybook to GitHub Pages [skip ci]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwicom/orbit.git",
    "directory": "packages/orbit-components"
  },
  "keywords": [
    "orbit",
    "components",
    "ui",
    "library",
    "component"
  ],
  "files": [
    "es",
    "lib",
    "umd",
    "icons",
    "orbit-icons-font",
    "orbit-icons-font.zip",
    "orbit-svgs.zip",
    "package.json",
    "readme.md",
    "yarn.lock"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "kiwi.com",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.12.0",
    "styled-components": "^4.4.1"
  },
  "dependencies": {
    "@adeira/js": "^1.3.0",
    "@kiwicom/orbit-design-tokens": "^1.0.1",
    "@popperjs/core": "^2.9.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "react-popper": "^2.2.5",
    "react-uid": "^2.3.1"
  },
  "devDependencies": {
    "@bunchtogether/vite-plugin-flow": "^1.0.1",
    "cpy-cli": "^3.1.1",
    "ora": "^6.0.1",
    "react-window": "^1.8.6",
    "vite": "^2.5.1",
    "zx": "^4.2.0"
  }
}
