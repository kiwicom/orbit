// @flow
export type Dimensions = {
  x: ?number,
  y: ?number,
  width: ?number,
  height: ?number,
  top: ?number,
  right: ?number,
  bottom: ?number,
  left: ?number,
};

export type UseBoundingRect = (
  initialValue: ?$Shape<Dimensions>,
) => [Dimensions, { current: ?HTMLElement }];

declare export default UseBoundingRect;
