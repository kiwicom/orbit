{
  "name": "@kiwicom/orbit-design-tokens",
  "version": "0.6.7",
  "description": "Design tokens for Kiwi.com.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiwicom/orbit-design-tokens.git"
  },
  "files": [
    "lib/*",
    "es/*",
    "package.json",
    "README.md",
    "output/*",
    "pages/*"
  ],
  "scripts": {
    "prepare": "yarn build && node ./src/theo/generate-theo-parser",
    "flow:check": "flow check",
    "build-html": "theo --setup ./src/theo/setup.js ./src/theo/theo-spec.json --format kiwi.html --dest ./pages",
    "build-web": "theo ./src/theo/theo-spec.json --transform web --format json,scss,sass,less,styl,cssmodules.css,common.js --dest ./output",
    "build-ios": "theo ./src/theo/theo-spec.json --transform ios --format ios.json --dest ./output",
    "build-android": "theo ./src/theo/theo-spec.json --transform android --format android.xml --dest ./output",
    "build:lib": "babel --config-file ./.babelrc --out-dir lib --ignore **/*.test.js,src/theo/* src && yarn copy:lib",
    "build:es": "babel --config-file ./.babel-es --no-babelrc --out-dir es --ignore **/*.test.js,src/theo/* src && yarn copy:es",
    "build": "yarn run build:lib && yarn run build-html && yarn run build-web && yarn run build-ios && yarn run build-android",
    "ci-publish": "ci-publish",
    "copy:lib": "copyfiles -u 1 'src/**/*.js.flow' lib",
    "copy:es": "copyfiles -u 1 'src/**/*.js.flow' es",
    "test": "jest",
    "eslint": "eslint .",
    "test-ci": "yarn flow:check && yarn eslint && yarn test --ci"
  },
  "author": "kiwi.com",
  "license": "MIT",
  "keywords": [
    "orbit",
    "ui",
    "design"
  ],
  "devDependencies": {
    "babel-plugin-replace-import-path": "^0.5.0",
    "babylon": "^6.18.0",
    "ci-publish": "^1.3.1",
    "copyfiles": "^2.1.1",
    "lodash": "^4.17.14",
    "prettier": "^1.18.2",
    "theo": "^8.1.2"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "bugs": {
    "url": "https://github.com/kiwicom/orbit-design-tokens/issues"
  },
  "homepage": "https://github.com/kiwicom/orbit-design-tokens#readme",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  }
}
