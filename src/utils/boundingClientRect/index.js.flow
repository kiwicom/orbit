// @flow
type Dimensions = {|
  top: number,
  right: number,
  bottom: number,
  left: number,
  height: number,
  width: number,
  pureTop: number,
  pureLeft: number,
  pureRight: number,
  pureBottom: number,
|};

export type BoundingClientRect = (ref: ?{ current: ?HTMLElement }) => ?Dimensions;

declare export default BoundingClientRect;
