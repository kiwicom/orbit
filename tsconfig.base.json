{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "esModuleInterop": true,
    "jsx": "react",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false
  },
  "ts-node": {
    "compilerOptions": {
      "noEmit": true,
      "module": "esnext",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "useUnknownInCatchVariables": false
    }
  }
}
